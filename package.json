{
  "name": "tanda",
  "version": "1.0.0",
  "description": "An isomorphic wrapper for Tanda's v2 API.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./out",
    "build:server": "babel src -d out",
    "build:client": "webpack",
    "build": "yarn run build:server && yarn run build:client",
    "lint": "eslint ./src",
    "test": "ava",
    "test:watch": "ava --watch",
    "prepublish": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deecewan/node-tanda.git"
  },
  "keywords": [
    "tanda"
  ],
  "author": "David Buchan-Swanson <david.buchanswanson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deecewan/node-tanda/issues"
  },
  "homepage": "https://github.com/deecewan/node-tanda#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^4.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3"
  }
}
